generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id                Int      @id @default(autoincrement())
  createdAt         DateTime @default(now())
  name              String   @unique
}

model Feedback {
  id                Int      @id @default(autoincrement())
  createdAt         DateTime @default(now())
  essay             String
  feedback          String?
  raw_request       Json
  raw_response      Json?
}